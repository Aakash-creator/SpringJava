package pattern;

public class Pyramid {

	public static void main(String[] args) {
		
		
int n  = 10;
		
		for(int i=0; i<n ; i++)
		{
			
			System.out.print("?");
//			for(int j = 0; j=)
//			System.out.println(" ");
			if(i>n-1 )
			{
				System.out.print("+");
			}
			System.out.println(" ");
			
			
		
		}
		
		

	}

}
